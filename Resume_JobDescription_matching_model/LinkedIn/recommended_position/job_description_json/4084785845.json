lue #InspireTrust\n\n\nFurther information is available at www.tuvsud.com \n\nTÜV SÜD AG:\nBoard of Management:\nJohannes Bussmann (Chairman)\nIshan Palit \n\nImprint: https://www.tuvsud.com/en/imprint \nData privacy: https://www.tuvsud.com/en/privacy-statement","$type":"com.linkedin.pemberly.text.AttributedText"},"jobPostingUrl":"https://www.linkedin.com/jobs/view/4084785845/?trk=jobs_biz_prem_srch","applies":128,"*allJobHiringTeamMembersInjectionResult":"urn:li:fs_jobHiringTeam:4084785845","workplaceTypesResolutionResults":{"*urn:li:fs_workplaceType:3":"urn:li:fs_workplaceType:3"},"poster":null,"dashEntityUrn":"urn:li:fsd_jobPosting:4084785845","*employmentStatusResolutionResult":"urn:li:fs_employmentStatus:FULL_TIME","eligibleForLearningCourseRecsUpsell":true,"eligibleForSharingProfileWithPoster":true,"trustReviewDecision":null,"originalListedAt":1732889713000,"entityUrn":"urn:li:fs_normalized_jobPosting:4084785845","workRemoteAllowed":false,"applyMethod":{"companyApplyUrl":"https://www.tuvsud.com/de-de/karriere/stellen/jobs/digital-transformation-manager-corporate-functions-business-support-muenchen-143213?utm_source=linkedin&utm_campaign=a-on_crawler_hr_de_co_cor_job_ts&utm_medium=paid&utm_content=143213","applyStartersPreferenceVoid":true,"inPageOffsiteApply":false,"$type":"com.linkedin.voyager.jobs.OffsiteApply"},"benefitsDataSource":"JOB_POSTER","*savingInfo":"urn:li:fs_jobSavingInfo:4084785845","draftApplicationInfo":null,"formattedJobFunctions":["Business Development","General Business","Administrative"],"messagingStatus":null,"dashJobPostingCardUrn":"urn:li:fsd_jobPostingCard:(4084785845,JOB_DETAILS)","locationVisibility":"ADDRESS","appeal":null,"expireAt":1739367511000,"$recipeTypes":["com.linkedin.voyager.deco.jobs.web.shared.WebFullJobPosting"],"$type":"com.linkedin.voyager.jobs.JobPosting","standardizedAddresses":null,"claimableByViewer":false,"inferredBenefits":null,"formattedIndustries":["International Trade and Development","Public Safety"],"indust