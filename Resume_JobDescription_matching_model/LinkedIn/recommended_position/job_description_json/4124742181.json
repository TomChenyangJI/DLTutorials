 well.\n\nhttps://www.brainlab.com/legal-notice/\nhttps://www.brainlab.com/privacy-policy/","staffCount":2131,"viewerFollowingJobsUpdates":false,"entityUrn":"urn:li:fs_normalized_company:9355","staffCountRange":{"start":1001,"end":5000,"$type":"com.linkedin.voyager.organization.shared.StaffCountRange"},"headquarter":{"geographicArea":"Bavaria","country":"DE","city":"Munich","postalCode":"81829","line1":"Olof-Palme-Straße 9","$type":"com.linkedin.common.Address"},"logo":{"image":{"$type":"com.linkedin.common.VectorImage","rootUrl":"https://media.licdn.com/dms/image/v2/C4D0BAQEgFqxz-vvJ8Q/company-logo_","artifacts":[{"width":200,"expiresAt":1744848000000,"fileIdentifyingUrlPathSegment":"200_200/company-logo_200_200/0/1630560132769/brainlab_logo?e=1744848000&v=beta&t=cI_h1aNvO2J5qMc3O9dnj28uKgBNUjBy-JnaxPzxqIU","$type":"com.linkedin.common.VectorArtifact","height":200},{"width":100,"expiresAt":1744848000000,"fileIdentifyingUrlPathSegment":"100_100/company-logo_100_100/0/1630560132769/brainlab_logo?e=1744848000&v=beta&t=GXE-vTxou7_n7jANBZnX3Aq8UOjdJX7mGOaYIXE9C3Y","$type":"com.linkedin.common.VectorArtifact","height":100},{"width":400,"expiresAt":1744848000000,"fileIdentifyingUrlPathSegment":"400_400/company-logo_400_400/0/1630560132769/brainlab_logo?e=1744848000&v=beta&t=7JR54s4mOHBWhNXmd7qd5_4k7OhifBTIFa82XTCjBmo","$type":"com.linkedin.common.VectorArtifact","height":400}]},"type":"SQUARE_LOGO","$type":"com.linkedin.voyager.organization.CompanyLogoImage"},"universalName":"brainlab","specialities":["Image-guided surgery software and platforms","Radiotherapy software and hardware","Cloud-based image enrichment and sharing","Digital O.R. integration","Radiosurgery software and hardware","Joint reconstruction software and hardware","Neurosurgery software and hardware","Spine and Trauma software and hardware"],"coverPhoto":{"$type":"com.linkedin.voyager.common.MediaProcessorImage","id":"/AAEAAQAAAAAAAAhDAAAAJGQ4NzNmYmE3LTg1OWEtNDcxMC1iY2MwLWI3YjdlNjM1NTlhYg.png"},"*followi